---
import data from '@/data/cards.json'
---

<section class="align-center flex justify-center">
  <h1>Welcome</h1>
  {
    data.map(resp => (
      <div>
        <b>{resp.id}</b>
        <h3>{resp.title}</h3>
        <img src={resp.img} alt="ticket" />
        <p>{resp.info}</p>
        <button>COMPRAR TICKET</button>
      </div>
    ))
  }
</section>
